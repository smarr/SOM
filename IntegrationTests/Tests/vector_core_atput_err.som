"
VM:
    status: error
    stdout:
        50
        ERROR: Vector[1..3]: Index 51 out of bounds

"

vector_core_atput_err = (
    run = (
        | vec |
        vec := Vector new.
        vec append: 10.
        vec append: 20.

        (vec capacity) println.

        vec at: 51 put: 10.

        (vec capacity) println.

        (vec at: 51) println.
    )
)